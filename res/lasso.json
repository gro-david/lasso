{
  "use_nixGL": false,
  "use_wal": true,
  "shell": "auto",
  "execute_commands": true,
  "app_dirs": [],
  "apps": [
    { "exec": "firefox", "name": "Firefox" },
    { "exec": "alacritty -e nvim", "name": "nvim" },
    { "exec": "alacritty -e ranger", "name": "ranger" },
    { "exec": "spotify", "name": "Spotify" }
  ],
  "dashboard": [
    { "exec": "reboot", "name": "Reboot" },
    { "exec": "shutdown now", "name": "Shutdown" },
    { "exec": "systemctl suspend", "name": "Suspend" },
    {
      "exec": "lasso-bluetooth",
      "name": "Bluetooth Management"
    },
    { "exec": "lasso-network", "name": "Network Management" },
    { "exec": "htop", "name": "System Monitor" }
  ]
}
